Table "address" {
    "id" unknown [note: 'Unique numeric identifier for the address.']
    "customer_id" unknown [note: '{{ doc(\'customer_id\') }}']
    "first_name" unknown [note: 'The customer\'s first name.']
    "last_name" unknown [note: 'The customer\'s last name.']
    "address_1" unknown [note: 'The street associated with the address.']
    "address_2" unknown [note: 'Any additional information associated with the address.']
    "city" unknown [note: 'The city associated with the address.']
    "province" unknown [note: 'The state or province associated with the address.']
    "country_code" unknown [note: 'The country code associated with the address.']
    "zip" unknown [note: 'The zip or postal code associated with the address.']
    "company" unknown [note: 'The company associated with the address.']
    "phone" unknown [note: 'The phone number associated with the address.']
    "created_at" unknown [note: 'The date and time when the address was created.']
    "updated_at" unknown [note: 'The date and time when the address was last updated.']
    "_fivetran_deleted" unknown [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "address_discounts" {
    "address_id" unknown [note: '{{ doc(\'address_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent address_id.']
    "id" unknown [note: '{{ doc(\'discount_id\') }}']
    "_fivetran_deleted" unknown [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "address_shipping_line" {
    "address_id" unknown [note: '{{ doc(\'address_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent address_id.']
    "price" unknown [note: 'The price of the shipping amount.']
    "code" unknown [note: 'The type of the shipping, can contain geographical shipping information.']
    "title" unknown [note: 'The type of shipping consignment.']
    "_fivetran_deleted" unknown [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']

Table "subscription" {
    "id" unknown [note: '{{ doc(\'subscription_id\') }}']
    "address_id" unknown [note: '{{ doc(\'address_id\') }}']
    "customer_id" unknown [note: '{{ doc(\'customer_id\') }}']
    "created_at" unknown [note: 'The date and time when the subscription was created.']
    "cancelled_at" unknown [note: 'The date and time when the subscription was cancelled.']
    "next_charge_scheduled_at" unknown [note: 'Date of the next charge for the subscription.']
    "price" unknown [note: 'The price of the item before discounts, taxes, or shipping have been applied.']
    "quantity" unknown [note: 'The number of items in the subscription.']
    "status" unknown [note: '{{ doc(\'subscription_status\') }}']
    "cancellation_reason" unknown [note: 'Reason provided for cancellation.']
    "cancellation_reason_comments" unknown [note: 'Additional comment for cancellation.']
    "product_title" unknown [note: 'The name of the product in a shop\'s catalog.']
    "variant_title" unknown [note: 'The name of the variant in a shop\'s catalog.']
    "external_product_id_ecommerce" unknown [note: 'Unique numeric identifier of the product in your external ecommerce platform.']
    "external_variant_id_ecommerce" unknown [note: 'Unique numeric identifier of the product variant in your external ecommerce platform.']
    "sku" unknown [note: 'A unique identifier of the item in fulfillment.']
    "order_interval_unit" unknown [note: 'The frequency with which a subscription should have the order created with (valid values are “day”, “week”, and “month”).']
    "order_interval_frequency" unknown [note: 'The number of units (specified in order_interval_unit) between each order, e.g. order_interval_unit = \'month\' and order_interval frequency = 3 indicates an order every 3 months.']
    "charge_interval_frequency" unknown [note: 'The number of units (specified in order_interval_unit) between each charge.']
    "order_day_of_week" unknown [note: 'The set day of the week order is created. This is only applicable to subscriptions with order_interval_unit = “week”.']
    "order_day_of_month" unknown [note: 'The set day of the month order is created. This is only applicable to subscriptions with order_interval_unit = “month”.']
    "expire_after_specific_number_of_charges" unknown [note: 'Set number of charges until subscription expires.']
    "updated_at" unknown [note: 'The date and time when the subscription was created.']
    "_fivetran_deleted" unknown [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "subscription_history" {
    "subscription_id" unknown [note: '{{ doc(\'subscription_id\') }}']
    "updated_at" unknown [note: 'The date time at which the purchase_item record was last updated.']
    "address_id" unknown [note: '{{ doc(\'address_id\') }}']
    "customer_id" unknown [note: '{{ doc(\'customer_id\') }}']
    "created_at" unknown [note: 'The date and time the subscription was created.']
    "cancelled_at" unknown [note: 'The date and time the subscription was cancelled.']
    "next_charge_scheduled_at" unknown [note: 'Date of the next charge for the subscription.']
    "price" unknown [note: 'The price of the item before discounts, taxes, or shipping have been applied.']
    "quantity" unknown [note: 'The number of items in the subscription.']
    "cancellation_reason" unknown [note: 'Reason provided for cancellation.']
    "status" unknown [note: '{{ doc(\'subscription_status\') }}']
    "cancellation_reason_comments" unknown [note: 'Additional comment for cancellation.']
    "product_title" unknown [note: 'The name of the product in a store’s catalog.']
    "variant_title" unknown [note: 'The name of the variant in a shop’s catalog.']
    "external_product_id_ecommerce" unknown [note: 'An object containing the product id as it appears in external platforms.']
    "external_variant_id_ecommerce" unknown [note: 'An object containing the variant id as it appears in external platforms.']
    "sku" unknown [note: 'A unique identifier of the item in the fulfillment. In cases where SKU is blank, it will be dynamically pulled whenever it is used.']
    "order_interval_unit" unknown [note: 'The frequency unit used to determine when a subscription’s order is created.']
    "order_interval_frequency" unknown [note: 'The number of units (specified in order_interval_unit) between each order. For example, order_interval_unit = month and order_interval_frequency = 3, indicate order every 3 months.']
    "charge_interval_frequency" unknown [note: 'The number of units (specified in order_interval_unit) between each Charge. For example, order_interval_unit=month and charge_interval_frequency=3, indicate charge every 3 months.']
    "order_day_of_week" unknown [note: 'The set day of the week order is created. Default is that there isn’t a strict day of the week order is created.']
    "order_day_of_month" unknown [note: 'The set day of the month order is created. Default is that there isn’t a strict day of the month when the order is created.']
    "expire_after_specific_number_of_charges" unknown [note: 'Set the number of charges until subscription expires.']
    "_fivetran_deleted" unknown [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}
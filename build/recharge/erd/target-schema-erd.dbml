Table "charge" {
    "id" unknown [note: '{{ doc(\'charge_id\') }}']
    "address_id" unknown [note: '{{ doc(\'address_id\') }}']
    "customer_id" unknown [note: '{{ doc(\'customer_id\') }}']
    "customer_hash" unknown [note: 'The hash of the customer associated with the charge.']
    "note" unknown [note: 'Shows the next order in sequence.']
    "created_at" unknown [note: 'Date and time when the charge was created.']
    "updated_at" unknown [note: 'Date and time when the charge was last updated.']
    "processed_at" unknown [note: 'Date and time when the charge was processed.']
    "scheduled_at" unknown [note: 'The date time of when the associated charge is/was scheduled to process.']
    "orders_count" unknown [note: 'The number of orders generated from this charge (Will be >1 for prepaid).']
    "external_order_id_ecommerce" unknown [note: 'The unique numeric identifier within your external ecommerce platform for the charge.']
    "subtotal_price" unknown [note: 'The combined price of all order line_items minus any discounts. Does not include taxes and shipping.']
    "tags" unknown [note: 'A comma-separated list of tags on the charge.']
    "tax_lines" unknown [note: 'An array of tax lines that apply to the charge.']
    "total_discounts" unknown [note: 'The sum of the discounts applied to the charge.']
    "total_line_items_price" unknown [note: 'The sum of all the prices of all the items in the charge.']
    "total_price" unknown [note: 'The sum of all the prices of all the items in the charge, taxes and discounts included (must be positive).']
    "total_tax" unknown [note: 'The total tax due associated with the charge.']
    "total_weight_grams" unknown [note: 'The total weight of all items in the charge in grams.']
    "type" unknown [note: '{{ doc(\'type\') }}']
    "status" unknown [note: '{{ doc(\'charge_status\') }}']
    "total_refunds" unknown [note: 'The sum of all refunds that were applied to the charge.']
    "external_transaction_id_payment_processor" unknown [note: 'The unique identifier of the transaction.']
    "email" unknown [note: 'The email address of the customer.']
    "payment_processor" unknown [note: 'The payment processor used on the charge.']
    "has_uncommited_changes" unknown [note: 'Specifies whether the charge is scheduled for regeneration (if the subscription is related to the charge was updated in the last 5 seconds using "commit_update":false).']
    "last_charge_attempt_date" unknown [note: 'The date when the charge was last attempted.']
    "retry_date" unknown [note: 'The date when the next attempt will be placed.']
    "error_type" unknown [note: 'Structured reason why the charge failed such as customer_needs_to_updated_card.']
    "error" unknown [note: 'Error reason as sentence text (typically returned directly from the payment processor - e.g. "customer needs to update credit card").']
    "charge_attempts" unknown [note: 'Shows how many times an attempt to charge was placed.']
    "external_variant_id_not_found" unknown [note: 'Indicates if Recharge was able to find the external_variant_id_ecommerce from the charge.']
    "client_details_browser_ip" unknown [note: 'The IP address of the buyer detected in checkout.']
    "client_details_user_agent" unknown [note: 'The user agent detected during checkout.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "charge_discount" {
    "charge_id" unknown [note: '{{ doc(\'charge_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent charge_id.']
    "id" unknown [note: '{{ doc(\'discount_id\') }}']
    "code" unknown [note: 'The code used to apply the discount.']
    "value" unknown [note: 'The discounted value to be applied.']
    "value_type" unknown [note: 'Possible values - FIXED_AMOUNT, PERCENTAGE, SHIPPING']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "charge_line_item" {
    "charge_id" unknown [note: '{{ doc(\'charge_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent charge_id.']
    "variant_title" unknown [note: 'The name of the product variant that links to your external ecommerce platform.']
    "title" unknown [note: 'The product title that links to your external ecommerce platform.']
    "grams" unknown [note: 'The weight of the charge\'s line item in grams.']
    "quantity" unknown [note: 'The quantity of the line_item.']
    "total_price" unknown [note: 'The total price of the line_item including tax.']
    "unit_price" unknown [note: 'The unit price of the line_item.']
    "tax_due" unknown [note: 'The total tax due associated with the line_item.']
    "taxable" unknown [note: 'A boolean indicating if the line_item is taxable or non-taxable.']
    "taxable_amount" unknown [note: 'The taxable revenue associated with the line_item.']
    "unit_price_includes_tax" unknown [note: 'A boolean indicator if tax is included in the price of an item.']
    "purchase_item_id" unknown [note: 'The subscription or onetime ID associated with the line_item.']
    "purchase_item_type" unknown [note: 'Possible values are SUBSCRIPTION, ONETIME.']
    "sku" unknown [note: 'The SKU (stock keeping unit) of the product associated with the charge\'s line item.']
    "external_product_id_ecommerce" unknown [note: 'The product ID that links to your external ecommerce platform.']
    "external_variant_id_ecommerce" unknown [note: 'The variant ID that links to your external ecommerce platform.']
    "vendor" unknown [note: 'The name of the seller.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "charge_order_attribute" {
    "charge_id" unknown [note: '{{ doc(\'charge_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent charge_id.']
    "order_attribute" unknown [note: 'An array of name-value pairs of order attributes on the charge.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "charge_shipping_line" {
    "charge_id" unknown [note: '{{ doc(\'charge_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent charge_id.']
    "price" unknown [note: 'The price of th shipping line of a charge.']
    "code" unknown [note: 'The code associated with the shipping line of a charge.']
    "title" unknown [note: 'The title of the shipping line.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "charge_tax_line" {
    "charge_id" unknown [note: '{{ doc(\'charge_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent charge_id.']
    "price" unknown [note: 'The total tax associated with the listed jurisdiction.']
    "rate" unknown [note: 'The tax rate associated with the listed jurisdiction.']
    "title" unknown [note: 'The title/name of the taxing jurisdiction.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "checkout" {
    "token" unknown [note: 'Unique token for the Checkout']
    "charge_id" unknown [note: 'ID for the Charge resulting from processing the Checkout.']
    "buyer_accepts_marketing" unknown [note: 'Boolean if the buyer accept marketing, newsletters etc.']
    "completed_at" unknown [note: 'Timestamp for when the Checkout was processed.']
    "created_at" unknown [note: 'Timestamp for when the Checkout was created.']
    "currency" unknown [note: 'Currency of the Checkout.']
    "discount_code" unknown [note: 'Discount code to be used on the checkout, e.g. “DISCOUNT20”.']
    "email" unknown [note: 'Email address for the customer.']
    "external_checkout_id" unknown [note: 'Represents the external cart token.']
    "external_checkout_source" unknown [note: 'Represents the source for external_checkout_id.']
    "external_transaction_id_payment_processor" unknown [note: 'Transaction ID of the external payment processor.']
    "order_attributes" unknown [note: 'Structured custom notes.']
    "phone" unknown [note: 'Customer phone number.']
    "requires_shipping" unknown [note: 'Boolean if the Checkout contains items that require shipping.']
    "subtotal_price" unknown [note: 'Value of the Checkout minus shipping and tax.']
    "taxes_included" unknown [note: 'Boolean if the tax is included in the price of the items.']
    "total_price" unknown [note: 'Full price of the Checkout including shipping and tax.']
    "total_tax" unknown [note: 'Tax charged on the Checkout.']
    "updated_at" unknown [note: 'Timestamp for the latest Checkout update.']
}

Table "customer" {
    "id" unknown [note: 'The unique ID of the customer.']
    "hash" unknown [note: 'The unique string identifier used in a customer portal link.']
    "external_customer_id_ecommerce" unknown [note: 'External platform\'s unique identifier for the customer.']
    "email" unknown [note: 'The email address of the customer.']
    "created_at" unknown [note: 'The date and time when the customer was created.']
    "updated_at" unknown [note: 'The date and time when the customer was last updated.']
    "first_charge_processed_at" unknown [note: 'Date when first charge was processed for the customer.']
    "first_name" unknown [note: 'The customer\'s first name.']
    "last_name" unknown [note: 'The customer\'s last name.']
    "subscriptions_active_count" unknown [note: 'The number of active subscriptions associated with the customer.']
    "subscriptions_total_count" unknown [note: 'The total number of subscriptions associated with the customer.']
    "has_valid_payment_method" unknown [note: 'Boolean indicating if the payment value is valid.']
    "has_payment_method_in_dunning" unknown [note: 'Boolean indicating if the customer has a credit card in dunning.']
    "tax_exempt" unknown [note: 'Boolean indicating if the customer is tax exempt.']
    "_fivetran_deleted" unknown [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "discount" {
    "id" unknown [note: '{{ doc(\'discount_id\') }}']
    "created_at" unknown [note: 'The date and time when the discount was created.']
    "updated_at" unknown [note: 'The date and time when the discount was last updated.']
    "starts_at" unknown [note: 'The date when the discount becomes active.']
    "ends_at" unknown [note: 'The expiration time of the discount, past this time the discount can no longer be redeemed, once the time of the discount has passed the status of the discount will go from \'active\' to \'disabled\'.']
    "code" unknown [note: 'The code used to apply the discount.']
    "value" unknown [note: 'The discounted value to be applied.']
    "status" unknown [note: '{{ doc(\'discount_status\') }}']
    "usage_limits" unknown [note: 'An integer indicating how many times the discount can been used.']
    "applies_to" unknown [note: 'Indicates where the discount applies.']
    "applies_to_resource" unknown [note: 'An indicator of the type of resource which applies_to_id refers.']
    "applies_to_id" unknown [note: 'A list of ids of the type indicated in applies_to_resource for which the discount can be applied.']
    "applies_to_product_type" unknown [note: 'Indicates which product types the discount applies to.']
    "minimum_order_amount" unknown [note: 'The minimum cart subtotal needed for the discount to be applicable. `duration` has to be `single_use` and the discount must apply to the entire order.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "one_time_product" {

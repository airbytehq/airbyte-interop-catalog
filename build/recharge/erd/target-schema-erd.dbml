Table "order_line_item" {
    "order_id" unknown [note: '{{ doc(\'order_id\') }}']
    "index" unknown [note: 'A unique numeric row produced for every concurrent order_id.']
    "variant_title" unknown [note: 'The title of the product variant.']
    "title" unknown [note: 'The title of the product.']

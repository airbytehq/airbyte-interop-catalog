    "price" unknown [note: 'The price of the item before discounts, taxes, or shipping have been applied.']
    "quantity" unknown [note: 'The number of items in the onetime purchase.']
    "external_product_id_ecommerce" unknown [note: 'The product ID that links to your external ecommerce platform.']
    "external_variant_id_ecommerce" unknown [note: 'The variant ID that links to your external ecommerce platform.']
    "sku" unknown [note: 'The unique identifier of the item in fulfillment.']
    "_fivetran_synced" unknown [note: '{{ doc(\'_fivetran_synced\') }}']
}


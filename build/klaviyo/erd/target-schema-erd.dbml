Table "campaign" {
    "_fivetran_deleted" varchar [note: 'Boolean that is true if the campaign has been soft-deleted.']
    "campaign_type" varchar [note: 'Type of campaign.']
    "created" varchar [note: 'Timestamp of when the campaign was created, in UTC.']
    "email_template_id" varchar [note: '''Foreign key referencing the ID of the `email_template` object that will be the  content of this campaign. Note the Email Template is copied when creating this campaign,  so future changes to that Email Template will not alter the content of this campaign.
    ''']
    "from_email" varchar [note: 'The email address your email will be sent from and will be used in the reply-to header.']
    "from_name" varchar [note: 'The name or label associated with the email address you\'re sending from.']
    "id" varchar [note: 'Unique ID of the campaign.']
    "name" varchar [note: 'A name for this campaign. If not specified, this will default to the subject of the campaign.']
    "send_time" varchar [note: '''Timestamp of when the campaign is scheduled to be sent in the future, if  ["smart send time"](https://help.klaviyo.com/hc/en-us/articles/360029794371-Smart-Send-Time-in-Klaviyo#how-to-utilize-smart-send-time3) is used. 
    ''']
    "sent_at" varchar [note: 'Timestamp of when the campaign was first sent out to users.']
    "status" varchar [note: 'Current status of the campaign. Either "draft", "scheduled", "sent", or "cancelled".']
    "status_id" varchar [note: 'Corresponding ID to the current status.']
    "status_label" varchar [note: 'The label of the status as it appears in the UI (should be the same as `status`).']
    "subject" varchar [note: 'The subject line of the campaign\'s email.']
    "updated" varchar [note: 'Timestamp of when the campaign was last updated.']
    "archived" varchar [note: 'Boolean of whether the campaign has been archived or not']
    "scheduled" varchar [note: 'The datetime when the campaign was scheduled for future sending']
}

Table "event" {
    "_variation" varchar [note: '''Unique ID of the attributed flow or campaign variation group.  This does not map onto another table. 
    ''']
    "campaign_id" varchar [note: 'Foreign key referencing the CAMPAIGN that the event is attributed to.']
    "datetime" varchar [note: 'Timestamp of when the event was recorded in Klaviyo. Should be the same/nominally off from `timestamp`.']
    "timestamp" varchar [note: 'Timestamp of when the event was triggered. Should be the same/nominally off from `datetime`.']
    "flow_id" varchar [note: 'Foreign key referencing the FLOW that the event is attributed to.']
    "flow_message_id" varchar [note: '''Unique ID of the FLOW_MESSAGE that the event is attributed to.  This does not map onto another table.
    ''']
    "id" varchar [note: 'Unique ID of the event.']
    "metric_id" varchar [note: 'Foreign key referencing the metric being captured.']
    "person_id" varchar [note: 'Foreign key referencing the PERSON who triggered the event.']
    "type" varchar [note: 'Type of event that was triggered. This is the same as the METRIC name.']
    "uuid" varchar [note: 'Universally Unique Identifier of the event.']
    "property_value" varchar [note: 'Numeric value associated with the event (ie the dollars associated with a purchase).']
    "_fivetran_deleted" varchar [note: 'Boolean that is true if the campaign has been soft-deleted.']
}

Table "flow" {
    "created" varchar [note: 'Timestamp of when the flow was first created.']
    "id" varchar [note: 'Unique ID of the flow.']
    "name" varchar [note: 'Name of the flow.']
    "status" varchar [note: 'Current status of the flow. Either \'manual\', \'live\', or \'draft\'. Read more [here](https://help.klaviyo.com/hc/en-us/articles/115002774932-Getting-Started-with-Flows#the-flow-action-status9).']
    "updated" varchar [note: 'Timestamp of when the flow was last updated.']
    "archived" varchar [note: 'Boolean of whether this record has been archived']
    "trigger_type" varchar [note: 'Corresponds to the object which triggered the flow.']
    "_fivetran_deleted" varchar [note: 'Boolean that is true if the campaign has been soft-deleted.']
}

Table "person" {
    "id" varchar [note: '''Unique ID of the user if you use your own unique identifier. Otherwise, Klaviyo  recommends using the email as the primary key. 
    ''']
    "address_1" varchar [note: 'First line of the person\'s address.']
    "address_2" varchar [note: 'Second line of the person\'s address.']
    "city" varchar [note: 'City they live in.']
    "country" varchar [note: 'Country they live in.']
    "zip" varchar [note: 'Postal code where they live.']
    "created" varchar [note: 'Timestamp of when the person\'s profile was created.']
    "email" varchar [note: 'The email address and the unique identifier for a profile.']
    "first_name" varchar [note: 'Person\'s first name.']
    "last_name" varchar [note: 'Person\'s surname.']
    "latitude" varchar [note: 'Latitude of the person\'s location.']
    "longitude" varchar [note: 'Longitude of the person\'s location.']
    "organization" varchar [note: 'Business organization they belong to.']
    "phone_number" varchar [note: 'Associated phone number.']
    "region" varchar [note: 'Region or state they live in.']
    "timezone" varchar [note: 'Timezone they are situated in.']
    "title" varchar [note: 'Title at their business or organization.']
    "updated" varchar [note: 'Timestamp of when the person profile was last updated.']
    "last_event_date" varchar [note: 'Date and time of the most recent event the triggered an update to the profile, in ISO 8601 format (YYYY-MM-DDTHH:MM:SS.mmmmmm)']
    "_fivetran_deleted" varchar [note: 'Boolean that is true if the campaign has been soft-deleted.']
}

Table "metric" {
    "created" varchar [note: 'Timestamp of when the metric was created.']
    "id" varchar [note: 'Unique ID of the metric being tracked.']
    "integration_id" varchar [note: 'Foreign key referencing the INTEGRATION that the metric is being pulled from.']
    "name" varchar [note: 'Name of the metric (same as `EVENT.type`)']
    "updated" varchar [note: 'Timestamp of when the metric was last updated.']
    "integration_category" varchar [note: 'Use-case category of the integrated platform.']
    "integration_name" varchar [note: 'Name of the integrated platform.']
    "_fivetran_deleted" varchar [note: 'Boolean that is true if the campaign has been soft-deleted.']
}
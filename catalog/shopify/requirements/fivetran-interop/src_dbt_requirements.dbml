    "customer_id" varchar [note: 'ID of the customer with the abandoned checkout.']
    "customer_locale" varchar [note: 'The two or three-letter language code, optionally followed by a region modifier. Example values - en, en-CA.']
    "device_id" varchar [note: 'The ID of the Shopify POS device that created the checkout.']
    "email" varchar [note: 'The customer\'s email address.']
    "gateway" varchar [note: 'The payment gateway used by the checkout.']
    "id" varchar [note: 'The ID for the checkout.']
    "landing_site_base_url" varchar [note: 'The URL for the page where the customer entered the shop.']
    "location_id" varchar [note: 'The ID of the physical location where the checkout was processed.']
    "name" varchar [note: 'Checkout order number.']
    "note" varchar [note: 'The text of an optional note that a shop owner can attach to the order.']
    "phone" varchar [note: 'The customer\'s phone number for receiving SMS notifications.']
    "presentment_currency" varchar [note: 'The three-letter code (ISO 4217 format) of the currency that the customer used at checkout. For the shop\'s default currency, see `currency`.']
    "referring_site" varchar [note: 'The website that referred the customer to the shop.']
    "shipping_address_address_1" varchar [note: 'The street address of the shipping address.']
    "shipping_address_address_2" varchar [note: 'An optional additional field for the street address of the shipping address.']
    "shipping_address_city" varchar [note: 'The city of the shipping address.']
    "shipping_address_company" varchar [note: 'The company of the person associated with the shipping address.']
    "shipping_address_country" varchar [note: 'The name of the country of the shipping address.']

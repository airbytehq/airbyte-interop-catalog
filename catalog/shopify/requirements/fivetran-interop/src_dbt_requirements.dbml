Table "order" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "app_id" varchar [note: 'The ID of the app that created the order.']
    "billing_address_address_1" varchar [note: 'The street address of the billing address.']
    "billing_address_address_2" varchar [note: 'An optional additional field for the street address of the billing address.']
    "billing_address_city" varchar [note: 'The city, town, or village of the billing address.']
    "billing_address_company" varchar [note: 'The company of the person associated with the billing address.']
    "billing_address_country" varchar [note: 'The name of the country of the billing address.']
    "billing_address_country_code" varchar [note: 'The two-letter code (ISO 3166-1 format) for the country of the billing address.']
    "billing_address_first_name" varchar [note: 'The first name of the person associated with the payment method.']
    "billing_address_last_name" varchar [note: 'The last name of the person associated with the payment method.']
    "billing_address_latitude" varchar [note: 'The latitude of the billing address.']
    "billing_address_longitude" varchar [note: 'The longitude of the billing address.']
    "billing_address_name" varchar [note: 'The full name of the person associated with the payment method.']
    "billing_address_phone" varchar [note: 'The phone number at the billing address.']
    "billing_address_province" varchar [note: 'The name of the region (province, state, prefecture, …) of the billing address.']
    "billing_address_province_code" varchar [note: 'The two-letter abbreviation of the region of the billing address.']
    "billing_address_zip" varchar [note: 'The postal code (zip, postcode, Eircode, …) of the billing address.']
    "browser_ip" varchar [note: 'The IP address of the browser used by the customer when they placed the order.']
    "buyer_accepts_marketing" varchar [note: 'Whether the customer consented to receive email updates from the shop.']
    "cancel_reason" varchar [note: 'The reason why the order was canceled.']
    "cancelled_at" varchar [note: 'The date and time when the order was canceled.']
    "cart_token" varchar [note: 'The ID of the cart that\'s associated with the order.']
    "closed_at" varchar [note: 'The date and time when the order was closed (archived).']
    "created_at" varchar [note: 'The autogenerated date and time when the order was created in Shopify.']
    "currency" varchar [note: 'The three-letter code for the shop currency.']
    "customer_id" varchar [note: 'The ID of the order\'s customer.']
    "email" varchar [note: 'The customer\'s email address.']
    "financial_status" varchar [note: 'The status of payments associated with the order. Can only be set when the order is created']
    "fulfillment_status" varchar [note: 'The order\'s status in terms of fulfilled line items.']
    "id" varchar [note: 'The ID of the order, used for API purposes. This is different from the order_number property, which is the ID used by the shop owner and customer.']
    "landing_site_base_url" varchar [note: 'The URL for the page where the buyer landed when they entered the shop.']
    "location_id" varchar [note: 'The ID of the physical location where the order was processed.']
    "name" varchar [note: 'The order name, generated by combining the order_number property with the order prefix and suffix that are set in the merchant\'s general settings.']
    "note" varchar [note: 'An optional note that a shop owner can attach to the order.']
    "number" varchar [note: 'The order\'s position in the shop\'s count of orders. Numbers are sequential and start at 1.']
    "order_number" varchar [note: 'The order \'s position in the shop\'s count of orders starting at 1001. Order numbers are sequential and start at 1001.']
    "processed_at" varchar [note: 'The date and time when an order was processed. This value is the date that appears on your orders and that\'s used in the analytic reports.']
    "referring_site" varchar [note: 'The website where the customer clicked a link to the shop.']
    "shipping_address_address_1" varchar [note: 'The street address of the shipping address.']
    "shipping_address_address_2" varchar [note: 'An optional additional field for the street address of the shipping address.']
    "shipping_address_city" varchar [note: 'The city, town, or village of the shipping address.']
    "shipping_address_company" varchar [note: 'The company of the person associated with the shipping address.']
    "shipping_address_country" varchar [note: 'The name of the country of the shipping address.']
    "shipping_address_country_code" varchar [note: 'The two-letter code (ISO 3166-1 format) for the country of the shipping address.']
    "shipping_address_first_name" varchar [note: 'The first name of the person associated with the shipping address.']
    "shipping_address_last_name" varchar [note: 'The last name of the person associated with the shipping address.']
    "shipping_address_latitude" varchar [note: 'The latitude of the shipping address.']
    "shipping_address_longitude" varchar [note: 'The longitude of the shipping address.']
    "shipping_address_name" varchar [note: 'The full name of the person associated with the payment method.']
    "shipping_address_phone" varchar [note: 'The phone number at the shipping address.']
    "shipping_address_province" varchar [note: 'The name of the region (province, state, prefecture, …) of the shipping address.']
    "shipping_address_province_code" varchar [note: 'The two-letter abbreviation of the region of the shipping address.']
    "shipping_address_zip" varchar [note: 'The postal code (zip, postcode, Eircode, …) of the shipping address.']
    "source_name" varchar [note: 'Where the order originated. Can be set only during order creation, and is not writeable afterwards.']
    "subtotal_price" varchar [note: 'The price of the order in the shop currency after discounts but before shipping, taxes, and tips in the shop currency.']
    "taxes_included" varchar [note: 'Whether taxes are included in the order subtotal.']
    "test" varchar [note: 'Whether this is a test order.']
    "token" varchar [note: 'A unique token for the order.']
    "total_discounts" varchar [note: 'The total discounts applied to the price of the order in the shop currency.']
    "total_line_items_price" varchar [note: 'The sum of all line item prices in the shop currency.']
    "total_price" varchar [note: 'The sum of all line item prices, discounts, shipping, taxes, and tips in the shop currency. Must be positive.']
    "total_tax" varchar [note: 'The sum of all the taxes applied to the order in th shop currency. Must be positive.']
    "total_weight" varchar [note: 'The sum of all line item weights in grams.']
    "updated_at" varchar [note: 'The date and time (ISO 8601 format) when the order was last modified.']
    "user_id" varchar [note: 'The ID of the user logged into Shopify POS who processed the order, if applicable.']
    "checkout_token" varchar [note: 'A unique value when referencing the checkout that\'s associated with the order.']
    "confirmed" varchar [note: 'Whether inventory has been reserved for the order.']
    "customer_locale" varchar [note: 'A two-letter or three-letter language code, optionally followed by a region modifier.']
    "checkout_id" varchar [note: 'ID of the order\'s checkout.']
    "order_status_url" varchar [note: 'The URL pointing to the order status web page, if applicable.']
    "_fivetran_deleted" varchar [note: '{{ doc(\'_fivetran_deleted\') }}']
    "total_tip_received" varchar [note: 'The sum of all the tips in the order in the shop currency.']
    "device_id" varchar [note: 'The ID for the device.']
    "presentment_currency" varchar [note: 'The presentment currency that was used to display prices to the customer.']
    "total_shipping_price_set" varchar [note: 'The total shipping price set for the order in presentment and shop currencies.']
    "client_details_user_agent" varchar [note: 'Details of the browsing client, including software and operating versions.']
    "total_tax_set" varchar [note: 'The total tax applied to the order in shop and presentment currencies.']
    "total_shipping_price_set" varchar [note: 'The total shipping price of the order, excluding discounts and returns, in shop and presentment currencies. If taxes_included is set to true, then total_shipping_price_set includes taxes.']
    "total_discounts_set" varchar [note: 'The total discounts applied to the price of the order in shop and presentment currencies.']
    "total_line_items_price_set" varchar [note: 'The total of all line item prices in shop and presentment currencies.']
    "total_price_set" varchar [note: 'The total price of the order in shop and presentment currencies.']
    "is_confirmed" varchar [note: 'Whether the order is confirmed.']
    "source_identifier" varchar [note: 'The ID of the order placed on the originating platform. This value doesn\'t correspond to the Shopify ID that\'s generated from a completed draft.']
}

Table "customer" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "accepts_marketing" varchar [note: 'Whether the customer has consented to receive marketing material via email. Deprecated and will be coalesced with `email_marketing_consent_state`.']
    "created_at" varchar [note: 'The date and time when the customer was created.']
    "default_address_id" varchar [note: 'The default address for the customer.']
    "email" varchar [note: 'The unique email address of the customer. Attempting to assign the same email address to multiple customers returns an error.']
    "first_name" varchar [note: 'The customer\'s first name.']
    "id" varchar [note: 'A unique identifier for the customer.']
    "last_name" varchar [note: 'The customer\'s last name.']
    "orders_count" varchar [note: 'The number of orders associated with this customer.']
    "phone" varchar [note: 'The unique phone number (E.164 format) for this customer. Attempting to assign the same phone number to multiple customers returns an error.']
    "state" varchar [note: 'The state of the customer\'s account with a shop.']
    "tax_exempt" varchar [note: 'Whether the customer is exempt from paying taxes on their order. If true, then taxes won\'t be applied to an order at checkout. If false, then taxes will be applied at checkout.']
    "total_spent" varchar [note: 'The total amount of money that the customer has spent across their order history.']
    "updated_at" varchar [note: 'The date and time when the customer information was last updated.']
    "verified_email" varchar [note: 'Whether the customer has verified their email address.']
    "email_marketing_consent_state" varchar [note: 'The current email marketing state for the customer. New version of `accepts_marketing` field.']
    "email_marketing_consent_opt_in_level" varchar [note: 'The marketing subscription opt-in level, as described in the M3AAWG Sender Best Common Practices, that the customer gave when they consented to receive marketing material by email. New version of `marketing_opt_in_level` field.']
    "email_marketing_consent_consent_updated_at" varchar [note: 'The date and time when the customer consented to receive marketing material by email. If no date is provided, then the date and time when the consent information was sent is used. New version of `accepts_marketing_updated_at` field.']
    "accepts_marketing_updated_at" varchar [note: 'Deprecated. The package will coalesce with `email_marketing_consent_consent_updated_at`.']
    "marketing_opt_in_level" varchar [note: 'Deprecated. The package will coalesce with `email_marketing_consent_opt_in_level`.']
    "_fivetran_deleted" varchar [note: '{{ doc(\'_fivetran_deleted\') }}']
    "note" varchar [note: 'A note about the customer.']
    "currency" varchar [note: 'The three-letter code (ISO 4217 format) for the currency that the customer used when they paid for their last order. Defaults to the shop currency. Returns the shop currency for test orders.']
}

Table "order_line" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "fulfillable_quantity" varchar [note: 'The amount available to fulfill, calculated as follows: quantity - max(refunded_quantity, fulfilled_quantity) - pending_fulfilled_quantity - open_fulfilled_quantity']
    "fulfillment_status" varchar [note: 'How far along an order is in terms line items fulfilled.']
    "gift_card" varchar [note: 'Whether the item is a gift card. If true, then the item is not taxed or considered for shipping charges.']
    "grams" varchar [note: 'The weight of the item in grams.']
    "id" varchar [note: 'The ID of the line item.']
    "name" varchar [note: 'The name of the product variant.']
    "order_id" varchar [note: 'The ID of the related order.']
    "price" varchar [note: 'The price of the item before discounts have been applied in the shop currency.']
    "product_id" varchar [note: 'The ID of the product that the line item belongs to. Can be null if the original product associated with the order is deleted at a later date.']
    "quantity" varchar [note: 'The number of items that were purchased.']
    "requires_shipping" varchar [note: 'Whether the item requires shipping.']
    "sku" varchar [note: 'The item\'s SKU (stock keeping unit).']
    "taxable" varchar [note: 'Whether the item was taxable.']
    "title" varchar [note: 'The title of the product.']
    "total_discount" varchar [note: 'The total amount of the discount allocated to the line item in the shop currency.']
    "variant_id" varchar [note: 'The ID of the product variant.']
    "vendor" varchar [note: 'The name of the item\'s supplier.']
    "index" varchar [note: 'Index of the order line.']
    "pre_tax_price" varchar [note: 'The pre tax price of the line item in shop currency.']
    "pre_tax_price_set" varchar [note: 'The pre tax price of the line item in shop currency and presentment currency.']
    "price_set" varchar [note: 'The price of the line item in shop and presentment currencies.']
    "tax_code" varchar [note: 'Tax code applied to the line item. As multiple taxes can apply to a line item, we recommend referring to `stg_shopify__tax_line`.']
    "total_discount_set" varchar [note: 'The total amount allocated to the line item in the presentment currency.']
    "variant_title" varchar [note: 'The title of the product variant.']
    "variant_inventory_management" varchar [note: 'The fulfillment service that tracks the number of items in stock for the product variant.']
    "properties" varchar [note: 'Line item properties.']
    "tax_code" varchar [note: 'Tax code applied to the line item. As multiple taxes can apply to a line item, we recommend referring to `stg_shopify__tax_line`.']
}

Table "order_line_refund" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "id" varchar [note: 'The unique identifier of the line item in the refund.']
    "location_id" varchar [note: 'TThe unique identifier of the location where the items will be restockedBD']
    "order_line_id" varchar [note: 'The ID of the related line item in the order.']
    "quantity" varchar [note: 'The quantity of the associated line item that was returned.']
    "refund_id" varchar [note: 'The ID of the related refund.']
    "restock_type" varchar [note: 'How this refund line item affects inventory levels.']
    "subtotal" varchar [note: 'Subtotal amount of the order line refund in shop currency.']
    "total_tax" varchar [note: 'The total tax applied to the refund in shop currency.']
    "subtotal_set" varchar [note: 'The subtotal of the refund line item in shop and presentment currencies.']
    "total_tax_set" varchar [note: 'The total tax of the line item in shop and presentment currencies.']
    "subtotal_set" varchar [note: 'The subtotal of the refund line item in shop and presentment currencies.']
}

Table "product" {
    "_fivetran_deleted" varchar [note: 'Whether the record has been deleted in the source system.']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "created_at" varchar [note: 'The date and time when the product was created.']
    "handle" varchar [note: 'A unique human-friendly string for the product. Automatically generated from the product\'s title.']
    "id" varchar [note: 'An unsigned 64-bit integer that\'s used as a unique identifier for the product. Each id is unique across the Shopify system. No two products will have the same id, even if they\'re from different shops.']
    "product_type" varchar [note: 'A categorization for the product used for filtering and searching products.']
    "published_at" varchar [note: 'The date and time (ISO 8601 format) when the product was published. Can be set to null to unpublish the product from the Online Store channel.']
    "published_scope" varchar [note: 'Whether the product is published to the Point of Sale channel.']
    "title" varchar [note: 'The name of the product.']
    "updated_at" varchar [note: 'The date and time when the product was last modified.']
    "vendor" varchar [note: 'The name of the product\'s vendor.']
    "status" varchar [note: '''The status of the product. Valid values: - active: The product is ready to sell and is available to customers on the online store, sales channels, and apps. By default, existing products are set to active. - archived: The product is no longer being sold and isn\'t available to customers on sales channels and apps. - draft: The product isn\'t ready to sell and is unavailable to customers on sales channels and apps. By default, duplicated and unarchived products are set to draft.
    ''']
}

Table "product_variant" {
    "barcode" varchar [note: 'The barcode, UPC, or ISBN number for the product.']
    "compare_at_price" varchar [note: 'The original price of the item before an adjustment or a sale in shop currency.']
    "created_at" varchar [note: 'The date and time (ISO 8601 format) when the product variant was created.']
    "fulfillment_service" varchar [note: '(DEPRECATED 2025-01-06) The fulfillment service associated with the product variant.']
    "grams" varchar [note: '(DEPRECATED 2025-01-06) The weight of the product variant in grams.']
    "id" varchar [note: 'The unique numeric identifier for the product variant.']
    "image_id" varchar [note: 'The unique numeric identifier for a product\'s image. The image must be associated to the same product as the variant.']
    "inventory_item_id" varchar [note: 'The unique identifier for the inventory item, which is used in the Inventory API to query for inventory information.']
    "inventory_management" varchar [note: '(DEPRECATED 2025-01-06) The fulfillment service that tracks the number of items in stock for the product variant.']
    "inventory_policy" varchar [note: 'Whether customers are allowed to place an order for the product variant when it\'s out of stock.']
    "inventory_quantity" varchar [note: 'An aggregate of inventory across all locations. To adjust inventory at a specific location, use the InventoryLevel resource.']
    "old_inventory_quantity" varchar [note: '(DEPRECATED 2025-01-06) Use the InventoryLevel resource instead.']
    "option_1" varchar [note: '''(DEPRECATED 2025-01-06) The custom properties that a shop owner uses to define product variants. You can define three options for a product variant: option1, option2, option3.
    ''']
    "option_2" varchar [note: '''(DEPRECATED 2025-01-06) The custom properties that a shop owner uses to define product variants. You can define three options for a product variant: option1, option2, option3.
    ''']
    "option_3" varchar [note: '''(DEPRECATED 2025-01-06) The custom properties that a shop owner uses to define product variants. You can define three options for a product variant: option1, option2, option3.
    ''']
    "position" varchar [note: 'The order of the product variant in the list of product variants. The first position in the list is 1. The position of variants is indicated by the order in which they are listed.']
    "price" varchar [note: 'The price of the product variant in shop currency.']
    "product_id" varchar [note: 'The unique numeric identifier for the product.']
    "requires_shipping" varchar [note: '(DEPRECATED 2025-01-06) Use the `requires_shipping` property on the InventoryItem resource instead.']
    "sku" varchar [note: 'A unique identifier for the product variant in the shop. Required in order to connect to a FulfillmentService.']
    "taxable" varchar [note: 'Whether a tax is charged when the product variant is sold.']
    "tax_code" varchar [note: 'This parameter applies only to the stores that have the Avalara AvaTax app installed. Specifies the Avalara tax code for the product variant.']
    "title" varchar [note: 'The title of the product variant. The title field is a concatenation of the option1, option2, and option3 fields. You can only update title indirectly using the option fields.']
    "updated_at" varchar [note: 'The date and time when the product variant was last modified. Gets returned in ISO 8601 format.']
    "weight" varchar [note: '(DEPRECATED 2025-01-06) The weight of the product variant in the unit system specified with weight_unit.']
    "weight_unit" varchar [note: '''(DEPRECATED 2025-01-06) The unit of measurement that applies to the product variant\'s weight. If you don\'t specify a value for weight_unit, then the shop\'s default unit of measurement is applied. Valid values: g, kg, oz, and lb.
    ''']
    "available_for_sale" varchar [note: 'Indicates whether the product variant is available for sale.']
    "display_name" varchar [note: 'The display name of the variant, based on the product\'s title and variant\'s title.']
    "legacy_resource_id" varchar [note: 'The ID of the corresponding resource in the REST Admin API.']
    "metafield" varchar [note: 'A custom field, including its namespace and key, that\'s associated with a Shopify resource for the purposes of adding and storing additional information.']
    "requires_components" varchar [note: 'Indicates whether a product variant requires components.']
    "sellable_online_quantity" varchar [note: 'The total sellable quantity of the variant for online channels.']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "transaction" {
    "id" varchar [note: 'The ID for the transaction.']
    "order_id" varchar [note: 'The ID for the order that the transaction is associated with.']
    "refund_id" varchar [note: 'The ID associated with a refund in the refund table.']
    "amount" varchar [note: 'The amount of money included in the transaction.']
    "authorization" varchar [note: 'The authorization code associated with the transaction.']
    "created_at" varchar [note: 'The date and time when the transaction was created.']
    "processed_at" varchar [note: 'The date and time when a transaction was processed.']
    "device_id" varchar [note: 'The ID for the device.']
    "gateway" varchar [note: 'The name of the gateway the transaction was issued through.']
    "source_name" varchar [note: 'The origin of the transaction.']
    "message" varchar [note: 'A string generated by the payment provider with additional information about why the transaction succeeded or failed.']
    "currency" varchar [note: 'The three-letter code (ISO 4217 format) for the currency used for the payment.']
    "location_id" varchar [note: 'The ID of the physical location where the transaction was processed.']
    "parent_id" varchar [note: 'The ID of an associated transaction.']
    "payment_avs_result_code" varchar [note: 'The response code from the address verification system.']
    "payment_credit_card_bin" varchar [note: 'The issuer identification number (IIN), formerly known as bank identification number (BIN) of the customer\'s credit card.']
    "payment_cvv_result_code" varchar [note: 'The response code from the credit card company indicating whether the customer entered the card security code, or card verification value, correctly.']
    "payment_credit_card_number" varchar [note: 'The customer\'s credit card number, with most of the leading digits redacted.']
    "payment_credit_card_company" varchar [note: 'The name of the company that issued the customer\'s credit card.']
    "kind" varchar [note: 'The transaction\'s type.']
    "receipt" varchar [note: 'A transaction receipt attached to the transaction by the gateway.']
    "currency_exchange_id" varchar [note: 'The ID of the adjustment.']
    "currency_exchange_adjustment" varchar [note: 'The difference between the amounts on the associated transaction and the parent transaction.']
    "currency_exchange_original_amount" varchar [note: 'The amount of the parent transaction in the shop currency.']
    "currency_exchange_final_amount" varchar [note: 'The amount of the associated transaction in the shop currency.']
    "currency_exchange_currency" varchar [note: 'The shop currency.']
    "error_code" varchar [note: 'A standardized error code, independent of the payment provider.']
    "status" varchar [note: 'The status of the transaction.']
    "test" varchar [note: 'Whether the transaction is a test transaction.']
    "user_id" varchar [note: 'The ID for the user who was logged into the Shopify POS device when the order was processed, if applicable.']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "authorization_expires_at" varchar [note: 'The date and time (ISO 8601 format) when the Shopify Payments authorization expires.']
}

Table "refund" {
    "id" varchar [note: 'The unique numeric identifier for the refund.']
    "created_at" varchar [note: 'Timestamp of the date when the refund was created.']
    "processed_at" varchar [note: 'Timestamp of the date when the refund was processed.']
    "note" varchar [note: 'User generated note attached to the refund.']
    "restock" varchar [note: 'Boolean indicating if the refund is a result of a restock.']
    "user_id" varchar [note: 'Reference to the user id which generated the refund.']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "total_duties_set" varchar [note: 'Record representing total duties set for the refund.']
    "order_id" varchar [note: 'Reference to the order which the refund is associated.']
}

Table "order_adjustment" {
    "id" varchar [note: 'The unique numeric identifier for the order adjustment.']
    "order_id" varchar [note: 'Reference to the order which the adjustment is associated.']
    "refund_id" varchar [note: 'Reference to the refund which the adjustment is associated.']
    "amount" varchar [note: 'Amount of the adjustment.']
    "tax_amount" varchar [note: 'Tax amount applied to the order adjustment in shop currency.']
    "kind" varchar [note: 'The kind of order adjustment (eg. refund, restock, etc.).']
    "reason" varchar [note: 'The reason for the order adjustment.']
    "amount_set" varchar [note: 'Amount set towards the order adjustment in shop and presentment currencies.']
    "tax_amount_set" varchar [note: 'Tax amount set towards the order adjustment in shop and presentment currencies.']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
}

Table "abandoned_checkout" {
    "_fivetran_deleted" varchar [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "abandoned_checkout_url" varchar [note: 'The recovery URL that\'s sent to a customer so they can recover their checkout.']
    "billing_address_address_1" varchar [note: 'The street address of the billing address.']
    "billing_address_address_2" varchar [note: 'An optional additional field for the street address of the billing address.']
    "billing_address_city" varchar [note: 'The city of the billing address.']
    "billing_address_company" varchar [note: 'The company of the person associated with the billing address.']
    "billing_address_country" varchar [note: 'The name of the country of the billing address.']
    "billing_address_country_code" varchar [note: 'The two-letter code (ISO 3166-1 alpha-2 format) for the country of the billing address.']
    "billing_address_first_name" varchar [note: 'The first name of the person associated with the payment method.']
    "billing_address_last_name" varchar [note: 'The last name of the person associated with the payment method.']
    "billing_address_latitude" varchar [note: 'The latitude of the billing address.']
    "billing_address_longitude" varchar [note: 'The longitude of the billing address.']
    "billing_address_name" varchar [note: 'The full name of the person associated with the payment method.']
    "billing_address_phone" varchar [note: 'The phone number at the billing address.']
    "billing_address_province" varchar [note: 'The name of the state or province of the billing address.']
    "billing_address_province_code" varchar [note: 'The two-letter abbreviation of the state or province of the billing address.']
    "billing_address_zip" varchar [note: 'The zip or postal code of the billing address.']

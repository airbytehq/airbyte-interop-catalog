    "variant_ids" varchar [note: '(DEPRECATED 2025-01-06) An array of variant ids associated with the image.']
    "width" varchar [note: 'Width dimension of the image which is determined on upload.']
    "alt_text" varchar [note: 'A word or phrase to share the nature or contents of an image.']
    "media_id" varchar [note: 'The unique identifier for the media associated with the product image.']
    "status" varchar [note: 'The status of the product image, indicating its availability or processing state.']
    "url" varchar [note: 'The URL of the product image.']
}

Table "product_tag" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "index" varchar [note: 'Index (starting from 1) represnting when the tag was placed on the product.']
    "product_id" varchar [note: 'ID of the product being tagged.']
    "value" varchar [note: 'Value of the tag.']
}

Table "shop" {
    "_fivetran_deleted" varchar [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "address_1" varchar [note: 'The shop\'s street address.']
    "address_2" varchar [note: 'The optional second line of the shop\'s street address.']
    "checkout_api_supported" varchar [note: 'Boolean representing whether the shop is capable of accepting payments directly through the Checkout API.']
    "city" varchar [note: 'The shop\'s city.']
    "cookie_consent_level" varchar [note: 'The cookie consent level defined on the shop\'s online store.']
    "country" varchar [note: 'The shop\'s country. In most cases, this value matches the country_code.']
    "country_code" varchar [note: 'The two-letter country code corresponding to the shop\'s country.']
    "country_name" varchar [note: 'The shop\'s normalized country name.']
    "county_taxes" varchar [note: 'Boolean representing whether the shop is applying taxes on a per-county basis. Only applicable to shops based in the US. Either `true` or `null` (not false, according to Shopify API docs).']
    "created_at" varchar [note: 'The date and time (ISO 8601) when the shop was created.']
    "currency" varchar [note: 'The three-letter code (ISO 4217 format) for the shop\'s default currency.']
    "customer_email" varchar [note: 'The contact email used for communication between the shop owner and the customer.']
    "domain" varchar [note: 'The shop\'s domain.']
    "eligible_for_card_reader_giveaway" varchar [note: 'Boolean representing whether the shop is eligible to receive a free credit card reader from Shopify.']
    "eligible_for_payments" varchar [note: 'Boolean representing whether the shop is eligible to use Shopify Payments.']
    "email" varchar [note: 'The contact email used for communication between Shopify and the shop owner.']
    "enabled_presentment_currencies" varchar [note: 'An array of of enabled currencies (ISO 4217 format) that the shop accepts. Merchants can enable currencies from their Shopify Payments settings in the Shopify Admin.']
    "force_ssl" varchar [note: 'DEPRECATED.']
    "google_apps_domain" varchar [note: 'The GSuite URL for the store, if applicable.']
    "google_apps_login_enabled" varchar [note: 'Boolean representing whether the GSuite login is enabled. Shops with this feature will be able to log in through the GSuite login page. Valid values are `true` and `null`.']
    "has_discounts" varchar [note: 'Boolean representing whether any active discounts exist for the shop.']
    "has_gift_cards" varchar [note: 'Boolean representing whether any active gift cards exist for the shop.']
    "has_storefront" varchar [note: 'Boolean representing whether this shop has an online store.']
    "iana_timezone" varchar [note: 'The name of the timezone assigned by the [IANA](https://www.iana.org/time-zones).']
    "id" varchar [note: 'The ID for the shop. A 64-bit unsigned integer.']
    "latitude" varchar [note: 'The latitude of the shop\'s location.']
    "longitude" varchar [note: 'The longitude of the shop\'s location.']
    "money_format" varchar [note: 'A string representing the way currency is formatted when the currency isn\'t specified.']
    "money_in_emails_format" varchar [note: 'A string representing the way currency is formatted in email notifications when the currency isn\'t specified.']
    "money_with_currency_format" varchar [note: 'A string representing the way currency is formatted when the currency is specified.']
    "money_with_currency_in_emails_format" varchar [note: 'A string representing the way currency is formatted in email notifications when the currency is specified.']
    "multi_location_enabled" varchar [note: 'DEPRECATED and hard-coded to `true`.']
    "myshopify_domain" varchar [note: 'The shop\'s .myshopify.com domain.']
    "name" varchar [note: 'The name of the shop.']
    "password_enabled" varchar [note: 'Boolean representing whether the password protection page is enabled on the shop\'s online store.']
    "phone" varchar [note: 'The contact phone number for the shop.']
    "plan_display_name" varchar [note: 'The display name of the Shopify plan the shop is on.']
    "plan_name" varchar [note: 'The name of the Shopify plan the shop is on.']
    "pre_launch_enabled" varchar [note: 'Boolen representing whether the pre-launch page is enabled on the shop\'s online store.']
    "primary_locale" varchar [note: 'The shop\'s primary locale, as configured in the language settings of the shop\'s theme.']
    "primary_location_id" varchar [note: 'DEPRECATED. Formerly used for the ID of the shipping origin location.']
    "province" varchar [note: 'The shop\'s normalized province or state name.']
    "province_code" varchar [note: 'The two- or three-letter code for the shop\'s province or state.']
    "requires_extra_payments_agreement" varchar [note: 'Boolean representing whether the shop requires an extra Shopify Payments agreement.']
    "setup_required" varchar [note: 'Boolean representing whether the shop has any outstanding setup steps.']
    "shop_owner" varchar [note: 'The username of the shop owner.']
    "source" varchar [note: 'The handle of the partner account that referred the merchant to Shopify, if applicable.']
    "tax_shipping" varchar [note: 'Boolean representing whether taxes are charged for shipping. Valid values are true or false.']
    "taxes_included" varchar [note: 'Boolean representing whether applicable taxes are included in product prices. Valid values are true or null.']
    "timezone" varchar [note: 'The name of the timezone the shop is in.']
    "updated_at" varchar [note: 'The date and time (ISO 8601) when the shop was last updated.']
    "weight_unit" varchar [note: 'The default unit of weight measurement for the shop.']
    "zip" varchar [note: 'The shop\'s zip or postal code.']
}

Table "tender_transaction" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "amount" varchar [note: 'The amount of the tender transaction in the shop\'s currency.']
    "currency" varchar [note: 'The three-letter code (ISO 4217 format) for the currency used for the tender transaction.']
    "id" varchar [note: 'The ID of the transaction.']
    "order_id" varchar [note: 'The ID of the order that the tender transaction belongs to.']
    "payment_method" varchar [note: '''Information about the payment method used for this transaction. Valid values include: - credit_card - cash - android_pay - apple_pay - google_pay - samsung_pay - shopify_pay - amazon - klarna - paypal - unknown - other
    ''']
    "processed_at" varchar [note: 'The date and time (ISO 8601 format) when the tender transaction was processed.']
    "remote_reference" varchar [note: 'The remote (gateway) reference associated with the tender.']
    "test" varchar [note: 'Whether the tender transaction is a test transaction.']
    "user_id" varchar [note: 'The ID of the user logged into the Shopify POS device that processed the tender transaction, if applicable.']
}

Table "abandoned_checkout_discount_code" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "amount" varchar [note: 'The amount of the discount in presentment currency.']
    "checkout_id" varchar [note: 'ID of the checkout.']
    "code" varchar [note: 'The discount code.']
    "created_at" varchar [note: 'When the checkout discount application was created.']
    "discount_id" varchar [note: 'ID of the discount. Deprecated, use `code` instead.']
    "index" varchar [note: 'Index (from 1) representing the application of the discount to the checkout. Use the latest (highest index)']
    "type" varchar [note: 'The type of discount. Valid values - percentage, shipping, fixed_amount. (default - fixed_amount)']
    "updated_at" varchar [note: 'When the checkout\'s discount was last updated']
}

Table "order_discount_code" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "amount" varchar [note: 'The amount that\'s deducted from the order total.']
    "code" varchar [note: 'This property returns the discount code that was entered at checkout. Otherwise this property returns the title of the discount that was applied.']
    "order_id" varchar [note: 'Associated order ID.']
    "type" varchar [note: 'The type of discount - `fixed_amount`, `percentage`, or `shipping`.']
    "index" varchar [note: 'Pairs with `order_id` to provide unique identifier for order discount code.']
}

Table "abandoned_checkout_shipping_line" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "carrier_identifier" varchar [note: 'A reference to the carrier service that provided the rate. Present when the rate was computed by a third-party carrier service.']
    "checkout_id" varchar [note: 'ID of the checkout that was abandoned.']
    "code" varchar [note: 'A reference to the shipping method.']
    "delivery_category" varchar [note: 'The general classification of the delivery method.']
    "delivery_expectation_range" varchar [note: 'Expected delivery date range.']
    "delivery_expectation_range_max" varchar [note: 'Latest expected delivery date.']
    "delivery_expectation_range_min" varchar [note: 'Earliest possible expected delivery date.']
    "delivery_expectation_type" varchar [note: 'Type of expected delivery.']
    "discounted_price" varchar [note: 'The pre-tax shipping price with discounts applied in _presentment_ (customer) currency.']
    "id" varchar [note: 'Unique ID of the abandoned checkout shipping line.']
    "index" varchar [note: 'Index of the line amongst shipping lines for this checkout.']
    "original_shop_price" varchar [note: 'The pre-tax shipping price without any discounts applied in _presentment_ (customer) currency.']
    "phone" varchar [note: 'The phone number at the shipping address.']
    "price" varchar [note: 'The price of the shipping method in presentment currency.']
    "requested_fulfillment_service_id" varchar [note: 'The fulfillment service requested for the shipping method. Present if the shipping method requires processing by a third party fulfillment service.']
    "source" varchar [note: 'The channel where the checkout originated. Example value - shopify.']
    "title" varchar [note: 'The title of the shipping method. Example value - International Shipping.']
}

Table "fulfillment_event" {
    "_fivetran_deleted" varchar [note: '{{ doc(\'_fivetran_deleted\') }}']
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "address_1" varchar [note: 'The street address where the fulfillment event occurred.']
    "city" varchar [note: 'The city where the fulfillment event occurred.']
    "country" varchar [note: 'The country where the fulfillment event occurred.']
    "created_at" varchar [note: 'The date and time (ISO 8601 format) when the fulfillment event was created.']
    "estimated_delivery_at" varchar [note: '''The estimated delivery date based on the fulfillment\'s tracking number, as long as it\'s provided by one of the following carriers: USPS, FedEx, UPS, or Canada Post (Canada only).  Value is `null` if no tracking number is available or if the tracking number is from an unsupported carrier. This property is available only when carrier calculated rates are in use.
    ''']
    "fulfillment_id" varchar [note: 'An ID for the fulfillment that\'s associated with the fulfillment event.']
    "happened_at" varchar [note: 'The date and time (ISO 8601 format) when the fulfillment event occurred.']
    "id" varchar [note: 'An ID for the fulfillment event.']
    "latitude" varchar [note: 'A geographic coordinate specifying the latitude of the fulfillment event.']
    "longitude" varchar [note: 'A geographic coordinate specifying the longitude of the fulfillment event.']
    "message" varchar [note: 'An arbitrary message describing the status. Can be provided by a shipping carrier.']
    "order_id" varchar [note: 'The ID of the order that\'s associated with the fulfillment event.']
    "province" varchar [note: 'The province where the fulfillment event occurred.']
    "shop_id" varchar [note: 'An ID for the shop that\'s associated with the fulfillment event.']
    "status" varchar [note: '''The status of the fulfillment event. Valid values: - label_printed: A label for the shipment was purchased and printed. - label_purchased: A label for the shipment was purchased, but not printed. - attempted_delivery: Delivery of the shipment was attempted, but unable to be completed. - ready_for_pickup: The shipment is ready for pickup at a shipping depot. - picked_up: The fulfillment was successfully picked up. - confirmed: The carrier is aware of the shipment, but hasn\'t received it yet. - in_transit: The shipment is being transported between shipping facilities on the way to its destination. - out_for_delivery: The shipment is being delivered to its final destination. - delivered: The shipment was successfully delivered. - failure: Something went wrong when pulling tracking information for the shipment, such as the tracking number was invalid or the shipment was canceled.
    ''']
    "updated_at" varchar [note: 'The date and time (ISO 8601 format) when the fulfillment event was updated.']
    "zip" varchar [note: 'The zip code of the location where the fulfillment event occurred.']
}

Table "tax_line" {
    "_fivetran_synced" varchar [note: '{{ doc(\'_fivetran_synced\') }}']
    "index" varchar [note: 'The index of the tax line.']
    "order_line_id" varchar [note: 'The order line that this tax line is associated with.']
    "price" varchar [note: 'The amount of tax, in shop currency, after discounts and before returns.']
    "price_set" varchar [note: 'The amount of tax, in shop and presentment currencies, after discounts and before returns.']
    "rate" varchar [note: 'The proportion of the line item price that the tax represents as a decimal.']
    "title" varchar [note: 'The name of the tax.']
}